#include <iostream>
#include <cstdio>

using namespace std;

char map[1024][1024];
int main()
{
	int n;
	cin>>n;

	for(int i = 0; i < n * 9; i++)
		scanf("%s", map[i]);

	for (int i = 0; i < n * 9; i++)
	{
		printf("%s",map[i]);
	}
	for(int i = 1; i < n * 9; i += 3)
	{
		printf("%s",map[i]);
	}
	for(int i = 2; i < n * 9; i += 3)
	{
	}
	return 0;
}
